<%= form_with(model: @master, local: true) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :store_name, "店名" %>
  <%= f.text_field :store_name, class: 'form-control' %>

  <%= f.label :user_name, "氏名" %>
  <%= f.text_field :user_name, class: 'form-control' %>

  <%= f.label :email, "メールアドレス" %>
  <%= f.email_field :email, class: 'form-control' %>

  <%= f.label :staff_number, "従業員番号" %>
  <%= f.number_field :staff_number, class: 'form-control' %>

  <%= f.label :password,"パスワード" %>
  <%= f.password_field :password, class: 'form-control' %>

  <%= f.label :password_confirmation,"確認用パスワード" %>
  <%= f.password_field :password_confirmation, class: 'form-control' %>
   

  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>